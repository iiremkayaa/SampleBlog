@model List<SharingListModel>

@foreach (var sharing in Model)
{
    <div class="card mb-4">
        <div class="card-body">
            <h2 class="card-title">@sharing.Title</h2>
            <p class="card-text">@sharing.Description</p>
        </div>
        <div class="card-footer text-muted">
            Posted on 02.01.2020
        </div>

    </div>
}
